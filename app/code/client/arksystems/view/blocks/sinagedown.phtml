<div class="row">
	<div class="col s12 center-align" id="sinagedown">
		<!-- Content will be automatically populated here. -->
	</div>
</div>
<script type="text/javascript">
$(document).ready(function() {
	var MyTimer2 = setInterval(function() {
		updateCollectionList();
	},  1000);
});

function updateCollectionList() {
	for( var i = 0; i < queueCollectionSinagedown.length; i++ ) {
		if (! $( "p#sinagedown-queue-" + queueCollectionSinagedown[i] ).length ) {
			newItem = $('<p class="qtxt" id="sinagedown-queue-'+ queueCollectionSinagedown[i] +'"><span>'+ queueCollectionSinagedown[i] +'</span></p>').hide();
			$("div#sinagedown").prepend(newItem);
			newItem.fadeIn();
		}
	}

	for( var x = 0; x < queueCompletedSinagedown.length; x++ ) {
		if( $( "p#sinagedown-queue-" + queueCompletedSinagedown[x] ).length ) {
			$( "p#sinagedown-queue-" + queueCompletedSinagedown[x] ).fadeOut(1000, function(){
				$( "p#sinagedown-queue-" + queueCompletedSinagedown[x] ).remove();
			});
			
		}
	}
}
</script>