<?php
$exception = $this->errors;
if( count($exception->getTrace()) < 1 ) {
	return;
}
?>

<h2>Traces</h2>
<div class="container" style="overflow-y: auto; height: 350px;">
	<?php
		foreach( $exception->getTrace() as $traces ) {
			echo "<pre>";
			foreach( $traces as $key => $val ) {
				?>
				 <?="\n"?><?=$key?>: <?=$val?> 
				<?php
			}
			echo "</pre>";
		}
	?>
</div>